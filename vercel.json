{
    "version": 2,
    "builds": [
      {
        "src": "dist/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "dist/api/$1.js"
      }
    ],
   "crons": [
  {
    "path": "/api/cron/etl",
    "schedule": "0 8 * * *"
  },
  {
    "path": "/api/cron/etl",
    "schedule": "0 20 * * *"
  }
]
  }
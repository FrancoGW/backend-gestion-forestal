{
    "version": 2,
    "builds": [
      {
        "src": "dist/api.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/api.js"
      }
    ],
    "crons": [
      {
        "path": "/api/cron/etl",
        "schedule": "0 * * * *"
      }
    ]
  }
{
    "version": 2,
    "builds": [
      { "src": "dist/api.js", "use": "@vercel/node" },
      { "src": "api/cron/etl.ts", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/cron/etl", "dest": "api/cron/etl.ts" },
      { "src": "/(.*)", "dest": "dist/api.js" }
    ],
    "crons": [
      {
        "path": "/api/cron/etl",
        "schedule": "0 8 * * *"
      }
    ]
  }